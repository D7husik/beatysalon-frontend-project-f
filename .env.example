# Environment Variables Example
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# ===========================================
# API Configuration
# ===========================================

# Base URL for the API (change for production)
VITE_API_BASE_URL=http://localhost:3000/api

# API timeout in milliseconds
VITE_API_TIMEOUT=10000

# ===========================================
# API Keys (if using external services)
# ===========================================

# Example: If using a real booking API
# VITE_BOOKING_API_KEY=your_api_key_here

# Example: If using Google Maps for location
# VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key_here

# Example: If using a payment gateway
# VITE_STRIPE_PUBLIC_KEY=pk_test_xxxxx

# ===========================================
# Feature Flags
# ===========================================

# Enable/disable mock API (true for development)
VITE_USE_MOCK_API=true

# Enable/disable debug mode
VITE_DEBUG_MODE=false
